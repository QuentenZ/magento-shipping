        <referenceContainer name="content">
            <referenceBlock name="sales.order.shipment">
                <referenceBlock name="shipment_items">
                    <action method="setTemplate">
                        <argument name="template" xsi:type="string">Cream_RedJePakketje::shipping/items.phtml</argument>
                    </action>
                </referenceBlock>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>
